# NextAuth 환경변수
NEXTAUTH_SECRET=your-secret-key-here
NEXTAUTH_URL=http://localhost:3000

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Kakao OAuth
AUTH_KAKAO_ID=your-kakao-app-id
AUTH_KAKAO_SECRET=your-kakao-app-secret

# Naver OAuth
AUTH_NAVER_ID=your-naver-client-id
AUTH_NAVER_SECRET=your-naver-client-secret

# MongoDB
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/youtube-search

# NAVER SMS (핸드폰 인증용)
NCP_SERVICE_ID=your-naver-cloud-service-id
NCP_ACCESS_KEY=your-naver-cloud-access-key
NCP_SECRET_KEY=your-naver-cloud-secret-key
NCP_PHONE_NUMBER=send-phone-number-format-01012345678

# YouTube API
NEXT_PUBLIC_YOUTUBE_API_KEY=your-youtube-api-key
